import reo.syncdrain;
import reo.sync;
import reo.fifo1;

main = (){ red(a) green(b) blue(c) alternator(a,b,c) }

red = (a!){ #PR identity(;a) | Java:"nl.cwi.reo.templates.Processes.Red" }

green = (a!){ #PR identity(;a) | Java:"nl.cwi.reo.templates.Processes.Green" }

blue = (a?){ #PR identity(a;) | Java:"nl.cwi.reo.templates.Processes.Blue" }

alternator = (a,b,c){ syncdrain(a, b) sync(b, x) fifo1(x, c) sync(a, c) }
