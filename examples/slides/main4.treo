section reo.slides;

import reo.sync;
import reo.lossy;

import reo.slides.red;
import reo.slides.green;
import reo.slides.blue;
import reo.slides.seqc;

main4() {
  green(a) red(b) blue(c) connector4(a, b, c)
}

connector4(a, b, c) {
  seqc(x, y) sync(a, x) sync(b, y) 
  lossy(m, c) sync(x, m) sync(y, m)
}
