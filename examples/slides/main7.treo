section reo.slides;

import reo.sync;
import reo.shiftlossyfifo1;

import reo.slides.red;
import reo.slides.green;
import reo.slides.blue;
import reo.slides.seqc;

main7() {
  green(a) red(b) blue(c) connector7(a, b, c)
}

connector7(a, b, c) {
  seqc(x, y) shiftlossyfifo1(a, x) shiftlossyfifo1(b, y) 
  sync(m, c) sync(x, m) sync(y, m)
}
