section reo.slides;

import reo.sync;
import reo.lossyfifo1;

import reo.slides.red;
import reo.slides.green;
import reo.slides.blue;
import reo.slides.seqc;

main6() {
  green(a) red(b) blue(c) connector6(a, b, c)
}

connector6(a, b, c) {
  seqc(x, y) lossyfifo1(a, x) lossyfifo1(b, y) 
  sync(m, c) sync(x, m) sync(y, m)
}
