section reo.slides;

import reo.fifo1;
import reo.sync;
import reo.lossy;

import reo.slides.red;
import reo.slides.green;
import reo.slides.blue;
import reo.slides.seqc;

main5() {
  green(a) red(b) blue(c) connector5(a, b, c)
}

connector5(a, b, c) {
  seqc(x, y) fifo1(a, x) fifo1(b, y) 
  lossy(m, c) sync(x, m) sync(y, m)
}
