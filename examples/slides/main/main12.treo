section reo.slides;

import reo.sync;
import reo.syncdrain;

import reo.slides.red;
import reo.slides.green;
import reo.slides.blue;
import reo.slides.seqc;

main12() {
  green(a) red(b) blue(c) connector12(a, b, c)
}

connector12(a, b, c) {
  seqc(x, y, z) sync(a, a1) sync(b, b1) sync(a1, c) sync(b1, c) 
  syncdrain(a1, a2) syncdrain(b1, z) sync(x, a2) sync(y, a2)
}













