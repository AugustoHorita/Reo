section reo.slides;

import reo.sync;

import slides.producer.red;
import slides.producer.green;
import slides.consumer.blue;
import slides.sequencer.seqc;

main10() {
  green(a[1]) /*...*/ red(a[n]) blue(b) 
  connector10<n>(a[1..n], b)
|
  n = 2
}

connector10<n:int>(a[1..n], b) {
  seqc(x[1..n]) 
  sync(a[i], x[i]) sync(x[i], m)
  sync(m, b)
|
  i : <1..n>
}
