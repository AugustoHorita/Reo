section reo.slides;

import reo.sync;

import reo.slides.red;
import reo.slides.green;
import reo.slides.blue;
import reo.slides.seqc;

main10() {
  green(a[1]) /*...*/ red(a[n]) blue(b) 
  connector10(a[1..n], b)
|
  n = 2
}

connector10(a[1..n], b) {
  seqc(x[1..n]) 
  sync(a[i], x[i]) sync(x[i], m)
  sync(m, b)
|
  i : <1..n>
}
