section reo.slides;

import reo.sync;
import reo.lossy;

import reo.slides.red;
import reo.slides.green;
import reo.slides.blue;
import reo.slides.seqc;

main3() {
  green(a) red(b) blue(c) connector3(a, b, c)
}

connector3(a, b, c) {
  seqc(x, y) lossy(a, x) lossy(b, y) 
  sync(m, c) sync(x, m) sync(y, m)
}




