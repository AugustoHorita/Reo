section reo.slides;

import reo.sync;
import reo.variable;

import reo.slides.red;
import reo.slides.green;
import reo.slides.blue;
import reo.slides.seqc;

main8() {
  green(a) red(b) blue(c) connector8(a, b, c)
}

connector8(a, b, c) {
  seqc(x, y) variable(a, x) variable(b, y) 
  sync(m, c) sync(x, m) sync(y, m)
}
