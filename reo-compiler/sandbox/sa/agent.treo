agent<id:String>(move,charge,readBat,readLoc,status){
	#RBA
	$m1 = "0";
	$m2 = "0";
	$m3 = "0";
	
	{move, readBat} move = "L", readBat != "0"
	{move, readBat} move = "D", readBat != "0"
