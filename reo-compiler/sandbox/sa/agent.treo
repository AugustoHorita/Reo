agent<id:String>(move!,agentId!){
	moveActions(lock1,lock2,move)
	getId<id>(agentId)
}	

getId<id:String>(agentId!){
	#RBA
	$m1 = id;
	agentId = $m1 , $m1'=$m1, agentId!=*
}

moveActions(lock1!,lock2!,move!){
	#RBA	
	move = s("L"), move != *, lock1=*, lock2!=*
	move = s("D"), move != *, lock1!=*, lock2=*
	move = s("U"), move != *, lock2!=*, lock1 !=*
	move = s("R"), move != *, lock2=*, lock1 =*
}
