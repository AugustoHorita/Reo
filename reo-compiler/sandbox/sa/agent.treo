agent<id:String>(move!,charge!,readBat?,readLoc?,status?){
	#RBA
	$mid = id;
	$m1 = "agent knowledge battery: 0";
	$m2 = "agent knowledge location: 0";
	$m3 = "agent knowledge goal status: 0";
	
	{move} move = "L"
	{move} move = "D"
	{move} move = "U"
	{move} move = "R"
	{charge} charge = $mid
	{readBat} $m1' = readBat, $m1 != *
	{readLoc} $m2' = readLoc, $m2 != *
	{status} $m3' = status, $m3 != *
}	