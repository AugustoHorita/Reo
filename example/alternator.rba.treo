fifo1(a?,b!) {
	#RBA
	{a, ~b} $m = null, $m' = a
	{~a, b} $m != null, b = $m, $m' = null
}

sync(a?,b!){
	#RBA
	{a,b} a=b
}

syncdrain(a?,b?){
	#RBA
	{a,b} true
}

producer(a!) {
	#RBA {a} true
	Java:"MyTestComponents.producer"
}

alternator(b[1]!) {
	{ syncdrain(a[i], a[i+1]) | i : <1..50> }
	{ fifo1(b[i+1], b[i]) | i : <1..50> }
	{ sync(a[i], b[i]) | i : <1..51> }
	{ producer(a[i]) | i : <1..51> }
}

